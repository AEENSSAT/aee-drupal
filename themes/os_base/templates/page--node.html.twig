<div id="wrapper" class="article">
    <header>
        <img src="../themes/os_base/imgs/LogoAEE-300.png" alt="">
        <nav>
            <ul>
                <li><a href="index.html">Accueil</a></li>
                <li><a href="categories.html">LES ACTUS</a></li>
                <li><a href="clubs.html">LES CLUBS</a></li>
                <li><a href="#">calendrier</a></li>
                <li><a href="#">Infos pratiques</a></li>
                <li><a href="login.html">Espace adherent</a></li>
            </ul>
        </nav>
    </header>

    <section role="main">
        <header>
            <img src="../themes/os_base/imgs/ploumanach.jpg" alt="" class="head">
        </header>

        <section class="content">
            {%
              set classes = [
                'node',
                'node--type-' ~ node.bundle|clean_class,
                node.isPromoted() ? 'node--promoted',
                node.isSticky() ? 'node--sticky',
                not node.isPublished() ? 'node--unpublished',
                view_mode ? 'node--view-mode-' ~ view_mode|clean_class,
                'clearfix',
              ]
            %}
            {{ attach_library('classy/node') }}
            <article{{ attributes.addClass(classes) }}>
              <header>
                {{ title_prefix }}
                {% if not page %}
                  <h2{{ title_attributes.addClass('node__title') }}>
                    <a href="{{ url }}" rel="bookmark">{{ label }}</a>
                  </h2>
                {% endif %}
                {{ title_suffix }}
                {% if display_submitted %}
                  <div class="node__meta">
                    {{ author_picture }}
                    <span{{ author_attributes }}>
                      {% trans %}Submitted by {{ author_name }} on {{ date }}{% endtrans %}
                    </span>
                    {{ metadata }}
                  </div>
                {% endif %}
              </header>
              <div{{ content_attributes.addClass('node__content', 'clearfix') }}>
                {{ content }}
              </div>
            </article>

            <aside>
                <div class="aside-part">
                    <form action="">
                        <input type="search">
                        <input type="submit">
                    </form>
                </div>


                <div class="aside-part">
                    <ul>
                        <li>Lorem</li>
                        <li>dolor</li>
                        <li>sit</li>
                        <li>amet</li>
                    </ul>
                </div>
            </aside>
        </section>
    </section>

    </div>
